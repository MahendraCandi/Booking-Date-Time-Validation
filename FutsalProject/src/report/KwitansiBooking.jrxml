	<queryString>
		<![CDATA[SELECT
b.diskon,
b.hari_libur,
b.jam_masuk,
b.jam_keluar,
b.kd_booking,
b.kd_lap,
b.kd_pelanggan,
b.kd_user,
b.tgl_booking,
b.tgl_pakai,
b.uang_dp,
dp.nm_pelanggan,
dl.jenis_lap,
dl.tarif
FROM booking b
INNER JOIN data_pelanggan dp ON b.kd_pelanggan = dp.kd_pelanggan
INNER JOIN data_lapangan dl ON b.kd_lap = dl.kd_lap
WHERE b.kd_booking = $P{kode_booking}]]>
	</queryString>
